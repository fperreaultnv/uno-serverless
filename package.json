    "test": "mocha -r ts-node/register tests/**/*-tests.ts",
    "cover": "nyc mocha -r ts-node/register -r source-map-support/register tests/**/*-tests.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julienblin/nv-aws-lambda.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julienblin/nv-aws-lambda/issues"
  },
  "homepage": "https://github.com/julienblin/nv-aws-lambda#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "mocha": "^5.1.1",
    "nyc": "^11.8.0",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.3",
    "http-status-codes": "^1.3.0",
    "source-map-support": "^0.5.6"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
