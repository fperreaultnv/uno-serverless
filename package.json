{
  "name": "nv-aws-lambda",
  "description": "An opiniated AWS Lambda/API Gateway/Node 8/Typescript framework",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "prebuild": "npm run lint",
    "build": "webpack --config webpack.config.js",
